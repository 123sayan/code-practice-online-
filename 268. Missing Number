class Solution {
    public int missingNumber(int[] nums) {
        int n=nums.length;
        int ans1=0;
        int ans2=0;
        for(int i=0;i<n;i++)
        {
            ans1+=nums[i];
        }
        for(int i=1;i<=n;i++)
        {
            ans2+=i;
        }
        return ans2-ans1;
    }
}
